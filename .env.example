# ============================================
# Sentynela Urban - Environment Variables
# Copy to .env and fill in values
# ============================================

# Database
POSTGRES_DB=urbanapp
POSTGRES_USER=urbanapp
POSTGRES_PASSWORD=CHANGE_ME_strong_password_here

# JWT - MUST be at least 32 characters, use: openssl rand -hex 32
JWT_SECRET=CHANGE_ME_run_openssl_rand_hex_32

# External APIs
OPENROUTESERVICE_API_KEY=your_ors_api_key_here

# CORS (JSON array of allowed origins)
CORS_ORIGINS=["http://localhost:3000","http://localhost:8081"]

# Environment: development | production
ENVIRONMENT=development
